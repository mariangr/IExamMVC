@{
    ViewBag.Title = "Tests";
}

<header>
    <script src="~/Scripts/knockout-3.1.0.debug.js"></script>
    <script src="~/Scripts/PagesScript/Tests/IExamTestsPageScript.js"></script>
</header>

<div class="container-fluid" id="tests_container">
    <div class="panel panel-success">
        <table class="table table-hover panel-success">
            <thead class="panel-heading">
                <tr>
                    <td>
                        Test Name
                    </td>
                    <td>
                        Start Test
                    </td>
                </tr>
            </thead>
            <tbody data-bind="foreach: tests">
                <tr>
                    <td data-bind="text: name">

                    </td>
                    <td >
                        <button class="btn btn-info" name="testId" data-bind="attr: {onclick: url }">Do Test</button>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>