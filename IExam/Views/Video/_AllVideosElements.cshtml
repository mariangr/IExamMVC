@model IEnumerable<IExam.Models.Video>

@if (Model.Count() > 0) {
<tbody class="col-md-12">
@foreach (var item in Model)
{
    <tr class="col-md-12">
        <td class="col-md-12">
            <input class="btn btn-info videoHeader col-sm-9 col-md-9" value="@item.name" type="button" onclick="LoadVideo(@item.ID)" />
            <input class="btn btn-danger videoDelete col-sm-3 col-md-3" value='Delete' type="button" onclick="DeleteVideo(@item.ID)">
        </td>
    </tr>
}
    
</tbody>
}
else {
    <tbody class="col-md-12">
        <tr class="col-md-12">
            <td class="col-md-12">
                <p class="alert-danger">No Videos.</p>
            </td>
        </tr>
    </tbody>
}
