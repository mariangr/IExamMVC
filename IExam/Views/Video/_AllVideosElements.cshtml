@model IEnumerable<IExam.Models.Video>

@if (Model.Count() > 0)
{
    <div class="row">
        @foreach (var item in Model)
        {
        <div class="col-md-12" style="padding-bottom: 5px;">
            <div class="btn btn-info videoHeader col-sm-9 col-md-9 wordwrap" id="multilineDiv" onclick="LoadVideo(@item.ID)" >@item.name</div>
            <input class="btn btn-danger videoDelete col-sm-3 col-md-3" value='Delete' type="button" onclick="DeleteVideo(@item.ID)">
        </div>
        }
    </div>
}
else
{
    <div class="col-md-12">
        <p class="alert-danger">No Videos.</p>
    </div>
}
